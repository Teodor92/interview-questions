{
    "name": "interview-questions",
    "version": "1.0.0",
    "description": "A list of interview questions.",
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
        }
    },
    "scripts": {
        "lint": "markdownlint \"./**/*.md\" --ignore \"node_modules/**\"",
        "release": "standard-version",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Teodor92/interview-questions.git"
    },
    "keywords": [
        "interview",
        "questions"
    ],
    "author": "Teodor Kurtev",
    "license": "MIT",
    "devDependencies": {
        "@commitlint/cli": "^18.4.4",
        "@commitlint/config-conventional": "^18.4.4",
        "husky": "^9.0.6",
        "markdownlint": "0.33.0",
        "markdownlint-cli": "0.38.0"
    }
}
